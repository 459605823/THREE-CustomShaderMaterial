{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["index.js", "src"],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "typescript": {
        "moduleRoot": "test"
    },
     "templates": {
      "better-docs": {
          "name": "THREE-CustomShaderMaterial",
          "title": "CSM | ",
          "navLinks": [
            {
                "label": "Github",
                "href": "https://github.com/FarazzShaikh/THREE-CustomShaderMaterial"
            },
            {
                "label": "Example",
                "href": "example/index.html"
            }
        ]
        }
    },
    "opts": {
        "template": "node_modules/better-docs",
        "destination": "docs",
        "recurse": true,
        "readme": "README.md"
    }
}